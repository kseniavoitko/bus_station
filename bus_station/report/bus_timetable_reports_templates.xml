<odoo>

    <template id="info">
        <span t-esc="doc.route_id"/>
        <br/>
        <span t-esc="doc.driver_id"/>
        <br/>
        <span t-esc="doc.transport_id"/>
        <br/>
        <span t-esc="doc.bus_number"/>
        <br/>
        <span t-esc="doc.seats"/>
        <br/>
        <span t-esc="doc.departure_datetime"/>
        <br/>
        <span t-esc="doc.arrival_datetime"/>
    </template>

    <template id="report_timetable">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="bus.info"/>
            </t>
        </t>
    </template>

</odoo>
